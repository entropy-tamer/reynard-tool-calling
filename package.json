{
  "name": "reynard-tool-calling",
  "version": "0.1.2",
  "description": "Development and runtime tools for Reynard applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "smoke": "tsx ./src/codemode/examples/smoke.ts",
    "demo": "tsx ./src/codemode/examples/tool-chaining.ts",
    "algorithms": "tsx ./src/codemode/examples/algorithms-demo.ts",
    "ecosystem": "tsx ./src/codemode/examples/ecosystem-scenarios.ts",
    "startup": "tsx ./src/codemode/examples/agent-startup-demo.ts",
    "list-tools": "tsx ./src/codemode/examples/list-tools.ts",
    "optimization": "tsx ./src/codemode/examples/development-optimization.ts",
    "advanced": "tsx ./src/codemode/examples/advanced-algorithm-uses.ts",
    "navigation": "tsx ./src/codemode/examples/spatial-navigation-demo.ts",
    "working-nav": "tsx ./src/codemode/examples/working-spatial-navigation.ts",
    "collision-test": "tsx ./src/codemode/examples/collision-detection-test.ts",
    "debug-collision": "tsx ./src/codemode/examples/debug-collision-issue.ts",
    "dev-tools": "timeout 60s tsx ./src/codemode/examples/dev-tools-integration-demo.ts",
    "dev-tools-simple": "timeout 30s tsx ./src/codemode/examples/dev-tools-simple-demo.ts",
    "practical": "timeout 30s tsx ./src/codemode/examples/practical-examples.ts",
    "automation": "timeout 30s tsx ./src/codemode/examples/development-automation.ts",
    "diagrams": "timeout 30s tsx ./src/codemode/examples/diagram-generation-demo.ts",
    "playwright": "timeout 60s tsx ./src/codemode/examples/playwright-demo.ts",
    "playwright-install": "playwright install chromium",
    "protein-analysis": "tsx ./src/codemode/examples/protein-analysis-demo.ts",
    "protein-analysis-simple": "tsx ./src/codemode/examples/protein-analysis-simple.ts"
  },
  "keywords": [
    "tools",
    "development",
    "runtime",
    "ai",
    "tool-calling",
    "reynard"
  ],
  "author": "Balazs Horvath",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "1.56.0",
    "dev-server-management": "workspace:*",
    "playwright": "1.56.0",
    "reynard-adr-system": "workspace:*",
    "reynard-algorithms": "workspace:*",
    "reynard-api-client": "workspace:*",
    "reynard-code-quality": "workspace:*",
    "reynard-dependency-graph": "workspace:*",
    "reynard-diagram-generator": "workspace:*",
    "reynard-file-processing": "workspace:*",
    "reynard-project-architecture": "workspace:*",
    "reynard-public-package-manager": "workspace:*",
    "reynard-queue-watcher": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.7.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-plugin-solid": "2.11.9",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
